entrypoint = "main.py"
modules = ["python-3.11", "python-3.13"]

[nix]
channel = "stable-25_05"
packages = ["unixtools.netstat"]

[unitTest]
language = "python3"

[gitHubImport]
requiredFiles = [".replit", "replit.nix"]

[deployment]
run = "python manage.py runserver 0.0.0.0:8000"
deploymentTarget = "cloudrun"

[agent]
expertMode = true

[[ports]]
localPort = 8000
externalPort = 80

[[ports]]
localPort = 8001
externalPort = 3000
